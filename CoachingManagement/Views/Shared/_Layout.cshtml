<html xmlns="http://www.w3.org/1999/xhtml">
<head>

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-reboot.css" rel="stylesheet" />

    <link href="~/lib/bootstrap/dist/css/bootstrap-grid.css" rel="stylesheet" />
    <title>@ViewBag.title</title>
</head>
<body>
    <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-item nav-link" asp-controller="Home" asp-action="Index">Home<span class="sr-only">(current)</span></a>
                    <a class="nav-item nav-link" asp-controller="Student" asp-action="CreateStudent">Add Student</a>
                    <a class="nav-item nav-link" asp-controller="Teacher" asp-action="CreateTeacher">Add Teacher</a>
                    <a class="nav-item nav-link" asp-controller="Course" asp-action="CreateCourse">Add Course</a>
                    <a class="nav-item nav-link" asp-controller="Teacher" asp-action="TeacherList">Teachers List</a>
                    <a class="nav-item nav-link" asp-controller="Course" asp-action="CourseList">Courses List</a>

                </div>
            </div>
        </nav>

        <div>

            @RenderBody()
        </div>
    </div>
</body>
</html>