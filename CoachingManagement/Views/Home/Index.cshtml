@model IEnumerable<Student>
@inject ICoachingRepository coaching


    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">StudentId</th>
                <th scope="col">Name</th>
                <th scope="col">FatherName</th>
                <th scope="col">Age</th>
                <th scope="col">Gender</th>
                <th scope="col">Course</th>
                <th scopre=" col" colspan="2">Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var el in Model)
            {

                <tr>
                    <td>@el.StudentId</td>
                    <td>@el.StudentName</td>
                    <td>@el.FatherName</td>
                    <td>@el.Age</td>
                    <td>@el.Gender</td>
                    <td>@coaching.CourseName(el)</td>
                    <td><a asp-action="Edit" asp-controller="Student" asp-route-id="@el.StudentId" class="btn btn-danger">Edit   </a></td>
                    <td><a asp-controller="student" asp-action="delete" asp-route-id="@el.StudentId" class="btn btn-danger">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>

    
